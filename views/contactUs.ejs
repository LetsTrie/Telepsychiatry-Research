<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('./partials/vendorsTop')%>
    <link rel="stylesheet" href="/css/contactUs.css" />
    <title>Forum | Telepsychiatry Research</title>
  </head>
  <body>
    <div style="min-height: 100vh;">
      <header>
        <nav class="cnav">
          <%- include('./partials/navbar')%>
        </nav>
        <div class="my-3 divider"></div>
      </header>

      <div class="contactUs">
        <div class="container">
          <div class="wrapper">
            <div class="contactUs_header text-center">
              <p>Contact Us</p>
            </div>
            <div class="contactUs_form">
              <form>
                <div class="flex-parentt">
                  <div class="flex-childd">
                    <p>Name :</p>
                    <input type="text" />
                  </div>

                  <div class="flex-childd">
                    <p>Email :</p>
                    <input type="email" />
                  </div>
                </div>
                <div class="messagee">
                  <p>Your message :</p>
                  <textarea
                    name="message"
                    id="message"
                    cols="30"
                    rows="10"
                  ></textarea>
                </div>
                <div class="submit">
                  <button>Submit</button>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>

    <%- include('./partials/footer')%>
  </body>
  <%- include('./partials/vendorsBottom')%>
</html>
