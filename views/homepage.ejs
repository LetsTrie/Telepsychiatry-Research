<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('./partials/vendorsTop')%>

  <link rel="stylesheet" href="/css/homepage.css" />
  <link rel="stylesheet" href="/css/homepage_services.css" />
  <link rel="stylesheet" href="/css/homepage_videoSection.css" />
  <link rel="stylesheet" href="/css/homepage_meetOurDoctors.css" />
  <link rel="stylesheet" href="/css/homepage_bestServices.css" />
  <link rel="stylesheet" href="/css/homepage_collaboration.css" />
  <link rel="stylesheet" href="/vendors/OwlCarousel2-2.3.4/dist/assets/owl.carousel.min.css" />
  <link rel="stylesheet" href="/vendors/OwlCarousel2-2.3.4/dist/assets/owl.theme.default.min.css" />

  <title>Homepage | Telepsychiatry Research</title>

  <style>
    .ourMainHomepage {
      position: relative;
    }

    .contactUsModal {
      z-index: 1;
      position: fixed;
      height: 80px;
      width: 80px;
      bottom: 80px;
      right: 60px;
      cursor: default;
      display: flex;
      flex-direction: center;
      justify-content: center;
      align-items: center;
      color: #228be6;
      border-radius: 50%;
      cursor: pointer;
      box-shadow: rgba(0, 0, 0, 0.2) 0px 6px 8px,
        inset rgba(0, 0, 0, 0.15) 0px -5px 10px;
      border: 45px solid rgb(251, 245, 245);
      background: white;
    }

    .bookAppointmentModal {
      z-index: 2;
      position: fixed;
      height: 50px;
      width: 200px;
      bottom: 10px;
      right: 10px;
      cursor: default;
      display: flex;
      flex-direction: center;
      justify-content: center;
      align-items: center;
    }

    .bookAppointmentModal button {
      background-color: #e74c3c;
      color: white;
      border: 3px solid rgb(186, 76, 76);
      border-radius: 5px;
      font-size: 18px;
      padding: 5px 10px 5px 10px;
    }

    .bookAppointmentModal button:hover {
      background-color: #ab2a1c;
    }

    @keyframes mymove {
      50% {
        outline-offset: 25px;
      }
    }

    .contactUsModal img {
      width: 90px;
      height: 80px;
      border-radius: 50%;
    }

    .imgg {
      outline: 5px solid coral;
      animation: mymove 2s infinite;
      box-shadow: rgba(0, 0, 0, 0.2) 0px 2px 7px,
        inset rgba(0, 0, 0, 0.15) 0px -5px 10px;
    }

    .magicc {
      position: relative;
    }

    .c-alert {
      margin-top: 20px;
      position: absolute;
      color: #155724;
      background-color: #d4edda;
      border-color: #c3e6cb;
      padding: 0.75rem 1.25rem;
      margin-bottom: 1rem;
      border: 1px solid transparent;
      border-radius: 0.25rem;
      width: 100%;
      text-align: center;
    }

    .c-alert p {
      font-size: 18px;
      margin-bottom: 5px;
    }
  </style>
</head>

<body>
  <%- include('./partials/homepage_contact_us_modal.ejs')%>
  <%-include('./partials/homepage_bookDoctor.ejs')%>

  <div class="ourMainHomepage">
    <div class="contactUsModal" data-toggle="modal" data-target="#HOMEPAGE_CONTACTUS_MODAL">
      <img src="/images/contactUsIcon2.png" alt="" />
    </div>

    <div class="bookAppointmentModal" data-toggle="modal" data-target="#HOMEPAGE_BOOK_APPOINTMENT_MODAL">
      <button>Book Appointment</button>
    </div>
    <!-- <div class="btnBook">
        <a
          href="#"
          data-toggle="modal"
          data-target="#HOMEPAGE_BOOK_APPOINTMENT_MODAL"
          >Book Appointments</a
        >
      </div> -->

    <div class="intro_page">
      <header class="intro_page_header">
        <%- include('./partials/navbar.ejs')%>
        <div class="wrapper magicc">
