    <script src="/vendors/OwlCarousel2-2.3.4/dist/owl.carousel.min.js"></script>
    <script src="/vendors/OwlCarousel2-2.3.4/dist/jquery.mousewheel.min.js"></script>
    <script>
      var owldl = $('.owl-doctor-list');
      owldl.owlCarousel({
        loop: true,
        margin: 10,
        autoplay: true,
        autoplayTimeout: 3000,
        autoplayHoverPause: true,
        dots: false,
        responsive: {
          0: {
            items: 1,
          },
          750: {
            items: 1,
          },
          960: {
            items: 2,
          },
          1100: {
            items: 3,
          },
        },
      });
      let owlorg = $('.owl-organizations');
      owlorg.owlCarousel({
        loop: true,
        margin: 10,
        autoplay: true,
        autoplayTimeout: 1300,
        autoplayHoverPause: true,
        dots: false,
        responsive: {
          0: {
            items: 2,
          },
          750: {
            items: 3,
          },
          960: {
            items: 3,
          },
          1100: {
            items: 6,
          },
        },
      });

      function gotoLink(url) {
        window.open(url, '_blank');
      }
    </script>
