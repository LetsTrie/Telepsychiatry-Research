    <title>Homepage | Telepsychiatry Research</title>

    <style>
      .ourMainHomepage {
        position: relative;
      }
      .contactUsModal {
        position: fixed;
        height: 80px;
        width: 80px;
        bottom: 60px;
        right: 60px;
        cursor: default;
        display: flex;
        flex-direction: center;
        justify-content: center;
        align-items: center;
        color: #228be6;
        border-radius: 50%;
        cursor: pointer;
        box-shadow: rgba(0, 0, 0, 0.2) 0px 6px 8px,
          inset rgba(0, 0, 0, 0.15) 0px -5px 10px;
        border: 45px solid rgb(251, 245, 245);
        background: white;
      }
      @keyframes mymove {
        50% {
          outline-offset: 25px;
        }
      }
      .contactUsModal img {
        width: 90px;
        border-radius: 50%;
      }
      .imgg {
        outline: 5px solid coral;
        animation: mymove 2s infinite;
        box-shadow: rgba(0, 0, 0, 0.2) 0px 2px 7px,
          inset rgba(0, 0, 0, 0.15) 0px -5px 10px;
