    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
    <script>
        $('#search').on('keypress', function(e) {
            if (e.which == 13) {
                // $('#form').submit()
            }
        });

        $('.see-more-btn').click(e => {
            window.location = '/services/singleInstitution';
        });

        $('#search-bar').on('keypress', (e) => {
            $('.each-package').hide()
            const phrase = $('#search-bar').val()
            const packages = $('.title p')
            for (let i = 0; i < packages.length; i++) {
                const line = packages[i].innerHTML.toLowerCase()
                if (line.includes(phrase)) {
                    $(packages[i].parentElement.parentElement).show()
                }
            }
        })
    </script>

