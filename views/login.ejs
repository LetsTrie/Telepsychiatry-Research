<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('./partials/vendorsTop')%>
    <link rel="stylesheet" href="/css/authentication.css" />
    <title>Login | Telepsychiatry Research</title>
  </head>
  <body>
    <div>
      <div style="min-height: 100vh;">
        <header>
          <nav>
            <%- include('./partials/navbar')%>
          </nav>
          <div class="my-3 divider"></div>
        </header>

        <section class="auth">
          <div class="auth_wrapper">
            <div class="auth_header">
              <h1>Login</h1>
            </div>

            <div class="auth_form">
              <div class="inputField">
                <div class="inputField_label">
                  <p>Email:</p>
                </div>
                <div class="inputField_textbox">
                  <input type="text" name="email" placeholder="Email Address" />
                </div>
              </div>

              <div class="inputField">
                <div class="inputField_label">
                  <p>Password:</p>
                </div>
                <div class="inputField_textbox">
                  <input
                    type="password"
                    name="password"
                    placeholder="Password"
                  />
                  <div class="forgetPassword">
                    <a
                      href="#"
                      data-toggle="modal"
                      data-target="#FORGET_PASSWORD_MODAL"
                    >
                      Forgot your password?
                    </a>
                  </div>
                </div>
              </div>

              <div class="submit">
                <button class="green-btn">Login</button>
              </div>

              <div class="signup">
                <div class="hr"></div>
                <div class="signup_ask">
                  <p>
                    Don't have an account?
                    <a
                      class="sign_up_href"
                      href="#"
                      data-toggle="modal"
                      data-target="#SELECT_CATEGORY_MODAL"
                      >Register</a
                    >
                  </p>
                </div>
              </div>
            </div>
          </div>
        </section>
      </div>
    </div>
    <%- include('./partials/register_forgetPassword') %> <%-
    include('./partials/register_selectCategory') %>
  </body>
  <%- include('./partials/vendorsBottom')%>

  <script></script>
</html>
