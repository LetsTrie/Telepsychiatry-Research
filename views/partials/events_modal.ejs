<div class="modal fade ourMODAL" id="WORKSHOP_REG_MODAL">
  <div
    class="modal-dialog modal-dialog-scrollable modal-lg modal-dialog-centered"
  >
    <div class="modal-content">
      <div class="modal-header">
        <h2 class="modal-title">Registration - Workshop</h2>
        <button type="button" class="close" data-dismiss="modal">×</button>
      </div>

      <div class="modal-body px-5">
        <div class="inputField">
          <div class="inputField_label">
            <p>Name :</p>
          </div>
          <div class="inputField_textbox">
            <input type="text" name="name" placeholder="Name" />
          </div>
        </div>
        <div class="inputField">
          <div class="inputField_label">
            <p>Email :</p>
          </div>
          <div class="inputField_textbox">
            <input type="text" name="email" placeholder="Email Address" />
          </div>
        </div>
        <div class="inputField">
          <div class="inputField_label">
            <p>Phone Number :</p>
          </div>
          <div class="inputField_textbox">
            <input type="text" name="pNumber" placeholder="Phone Number" />
          </div>
        </div>

        <div class="modal-body-submit submit">
          <button class="green-btn gotoResetPass">Register</button>
        </div>
      </div>

      <div class="modal-footer submit">
        <button type="button" class="red-btn" data-dismiss="modal">
          Close
        </button>
      </div>
    </div>
  </div>
</div>

