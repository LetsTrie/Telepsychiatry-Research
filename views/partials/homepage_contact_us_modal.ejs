<div class="modal fade" id="HOMEPAGE_CONTACTUS_MODAL">
  <div class="modal-dialog modal-dialog-scrollable modal-lg modal-dialog-centered">
    <div class="modal-content">
      <div class="modal-header">
        <h2 class="modal-title">Contact Us</h2>
        <button type="button" class="close" data-dismiss="modal">Ã—</button>
      </div>

      <div class="modal-body">
        <div class="wrapper">
          <div class="contactUs_form">
            <div class="alert alert-danger" id="alert"></div>
            <form id="form" action="/contactUs" method="POST">
              <% if(successMessage != ''){ %>
              <div class="alert alert-success" role="alert">
                <%= successMessage %>
              </div>
              <% } %>
              <div class="flex-parentt">
                <div class="flex-childd">
                  <p>Name :</p>
                  <input id="name" type="text" required name="name" />
                </div>

                <div class="flex-childd">
                  <p>Age :</p>
                  <input id="age" type="integer" required name="age" />
                </div>

                <div class="flex-childd">
                  <p>Gender :</p>
                  <input type="radio" id="male" name="gender" value="male">
                  <label for="male">Male</label>
                  <input type="radio" id="female" name="gender" value="female">
                  <label for="female">Female</label>
                  <input type="radio" id="other" name="gender" value="other">
                  <label for="other">Other</label>
                  <p>Seek help from :</p>
                  <input type="radio" id="psychiatric consultation" name="experts" value="psychiatric consultation">
                  <label for="psychiatric consultation">psychiatric consultation</label><br>
                  <input type="radio" id="psychology and counselling" name="experts" value="psychology and counselling">
                  <label for="psychology and counselling">psychology and counselling</label>
                </div>




                <div class="flex-childd">
                  <p>Mobile :</p>
                  <input id="mobile" type="text" required name="mobile" />
                </div>

                <div class="flex-childd">
                  <p>Email :</p>
                  <input id="email" type="email" required name="email" />
                </div>
              </div>
              <div class="messagee">
                <p>Your message :</p>
                <textarea required id="message" name="message" id="message" cols="30" rows="10"></textarea>
              </div>
              <div class="submit">
                <button id="submit-btn">Submit</button>
              </div>
            </form>
          </div>
        </div>
      </div>

      <div class="modal-footer submit">
        <button type="button" class="red-btn" data-dismiss="modal">
          Close
        </button>
      </div>
    </div>
  </div>
</div>