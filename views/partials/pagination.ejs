<div class="custom_pagination">
    <div class="myPagination">
        <% if(hasPreviousPage) { %>
            <a href="<%=URL%><%=previousPage%>" class="arrow">&laquo;</a>
            <% } else { %>
                <a href="#" class="arrow disabledBTN">&laquo;</a>
                <% } if(currentPage >= 5) { %>
                    <a href="<%=URL%>1">1</a>
                    <a href="<%=URL%>2">2</a>
                    <a href="#">...</a>
                    <a href="<%=URL%><%=currentPage - 1%>">
                        <%=currentPage - 1%>
                    </a>
                    <% } else { for(let i = 1 ; i < currentPage; i++) { %>
                        <a href="<%=URL%><%=i%>">
                            <%=i%>
                        </a>
                        <% } } %>
                            <a href="<%=URL%><%=currentPage%>" class="active">
                                <%=currentPage%>
                            </a>
                            <% if(currentPage + 5 > lastPage) { for(let i = currentPage + 1; i <=
    lastPage; i++) { %>
                                <a href="<%=URL%><%=i%>">
                                    <%=i%>
                                </a>
                                <% } } else { %>
                                    <a href="<%=URL%><%=currentPage + 1%>">
                                        <%=currentPage + 1%>
                                    </a>
                                    <a href="#">...</a>
                                    <a href="<%=URL%><%=lastPage - 1%>">
                                        <%=lastPage - 1%>
                                    </a>
                                    <a href="<%=URL%><%=lastPage%>">
                                        <%=lastPage%>
                                    </a>

                                    <% } if(hasNextPage) { %>
                                        <a href="<%=URL%><%=nextPage%>" class="arrow">&raquo;</a>
                                        <% } else { %>
                                            <a href="#" class="arrow disabledBTN">&raquo;</a>
                                            <% } %>
    </div>
</div>